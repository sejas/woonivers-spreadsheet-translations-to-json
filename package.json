{
  "name": "@woonivers/woo-translate",
  "version": "0.0.6",
  "description": "woo-translate CLI",
  "bin": {
    "woo-translate": "bin/woo-translate"
  },
  "scripts": {
    "format": "prettier --write **/*.{js,ts,tsx,json}",
    "lint": "tslint -p .",
    "compile": "tsc -p .",
    "build": "yarn format && yarn lint && yarn compile",
    "test": "jest",
    "watch": "jest --watch",
    "snapupdate": "jest --updateSnapshot",
    "coverage": "jest --coverage",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint"
  },
  "files": [
    "tsconfig.json",
    "tslint.json",
    "build",
    "core",
    "LICENSE",
    "readme.md",
    "docs",
    "bin"
  ],
  "license": "MIT",
  "dependencies": {
    "typescript": "3.2.2",
    "ts-node": "7.0.1",
    "gluegun": "2.1.2",
    "googleapis": "27.0.0",
    "node-fetch": "2.3.0"
  },
  "devDependencies": {
    "@types/node": "10.12.27",
    "@types/jest": "23.3.14",
    "ts-jest": "23.10.5",
    "tslint": "5.13.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-config-standard": "8.0.1",
    "prettier": "1.16.4",
    "jest": "23.6.0",
    "@types/gapi": "0.0.36",
    "@types/node-fetch": "2.1.6"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
}
